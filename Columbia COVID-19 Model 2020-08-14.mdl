regulations start time=
	VOLUNTARY PRECAUTIONS START TIME+VOLUNTARY PRECAUTIONS DURATION
	~	
	~	On this day, the government realizes COVID is a problem and implements \
		regulations.
	|

precautions effectiveness change rate=
	IF THEN ELSE(Time<VOLUNTARY PRECAUTIONS START TIME, 0 , IF THEN ELSE(Time<=regulations start time\
		,(VOLUNTARY PRECAUTIONS EFFECTIVENESS-Precautions Effectiveness)/TIME TO IMPLEMENT VOLUNTARY PRECAUTIONS\
		,IF THEN ELSE(Time<=(regulations start time+REGULATIONS DURATION), (REGULATED PRECAUTIONS EFFECTIVENESS\
		-Precautions Effectiveness)/TIME TO IMPLEMENT REGULATED PRECAUTIONS,(VOLUNTARY PRECAUTIONS EFFECTIVENESS\
		-Precautions Effectiveness)/TIME TO IMPLEMENT VOLUNTARY PRECAUTIONS) ) )
	~	fraction/day [-1,1]
	~		|

VOLUNTARY PRECAUTIONS DURATION=
	76
	~	days
	~	This is the time between when the citizens realize there is a problem with \
		COVID and when the government realizes there is a problem with COVID
	|

reproduction rate=
	daily reproduction rate*(1-Precautions Effectiveness)
	~	people/person/day [0,1]
	~		|

change in fatality fraction=
	IF THEN ELSE(Time<=VOLUNTARY PRECAUTIONS START TIME, 0 , (LONG TERM FATALITY FRACTION\
		-Fatality Fraction)/TIME TO CHANGE FATALITY FRACTION)
	~	fraction/day [-1,1]
	~		|

comfirmed cases=
	Deaths+Infected+Recovered
	~	people [0,?]
	~		|

confirmed deaths=
	Deaths
	~	people [0,?]
	~		|

controlled reproduction rate=
	reproduction rate*ILLNESS DURATION
	~	people/person [0,10]
	~		|

daily reproduction rate=
	UNCONTROLLED REPRODUCTION RATE/ILLNESS DURATION
	~	people/person/day [0,1]
	~		|

REGULATED PRECAUTIONS EFFECTIVENESS=
	0.4
	~	fraction [0,1]
	~		|

dying=
	Infected*Fatality Fraction/ILLNESS DURATION
	~	people/day [0,?]
	~		|

Fatality Fraction= INTEG (
	change in fatality fraction,
		INITIAL FATALITY FRACTION)
	~	fraction [0,1]
	~		|

FRACTION OF POPULATION TO REACH HERD IMMUNITY=
	0.7
	~	fraction [0,1]
	~		|

ILLNESS DURATION=
	10
	~	days [1,21]
	~	This is an assumed value.
	|

Infected= INTEG (
	infecting-dying-recovering,
		INITIAL INFECTED)
	~	people [0,?]
	~		|

infecting=
	(Susceptible/INITIAL SUSCEPTIBLE)*Infected*reproduction rate
	~	people/day [0,?]
	~		|

INITIAL FATALITY FRACTION=
	0.0576
	~	fraction
	~		|

INITIAL INFECTED=
	1
	~	people
	~		|

INITIAL SUSCEPTIBLE=
	POPULATION*FRACTION OF POPULATION TO REACH HERD IMMUNITY/UNCOMFIRMED CASE RATIO
	~	people [0,?]
	~		|

LONG TERM FATALITY FRACTION=
	0
	~	fraction [0,1]
	~		|

VOLUNTARY PRECAUTIONS EFFECTIVENESS=
	0.336
	~	fraction [0,1]
	~		|

POPULATION=
	5.09e+007
	~	people [0,?]
	~		|

TIME TO CHANGE FATALITY FRACTION=
	160
	~	days [1,?]
	~		|

REGULATIONS DURATION=
	30
	~	days [1,?]
	~	This is the number of days in which the regulations are in place.
	|

recovering=
	Infected*(1-Fatality Fraction)/ILLNESS DURATION
	~	people/day [0,?]
	~		|

UNCOMFIRMED CASE RATIO=
	10
	~	fraction
	~		|

Susceptible= INTEG (
	-infecting,
		INITIAL SUSCEPTIBLE)
	~	people [0,?]
	~		|

TIME TO IMPLEMENT REGULATED PRECAUTIONS=
	20
	~	days [1,?]
	~		|

Precautions Effectiveness= INTEG (
	precautions effectiveness change rate,
		0)
	~	fraction [0,1]
	~		|

Deaths= INTEG (
	dying,
		0)
	~	people [0,?]
	~		|

TIME TO IMPLEMENT VOLUNTARY PRECAUTIONS=
	1
	~	days [1,?]
	~		|

Recovered= INTEG (
	recovering,
		0)
	~	people [0,?]
	~		|

VOLUNTARY PRECAUTIONS START TIME=
	85
	~	days [1,?]
	~	On this day, the population notices the COVID is a problem and begins to \
		take voluntary precautions.
	|

UNCONTROLLED REPRODUCTION RATE=
	2.07
	~	dmnl [1,10]
	~		|

